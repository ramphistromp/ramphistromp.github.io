          "_name": "testimonials1",
          "_sourceTheme": "attorneym4",
          "_customHTML": "<section class=\"carousel testimonials1 attorneym4_testimonials1 testimonials-slider\" group=\"Testimonials\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\" plugins=\"BootstrapCarouselSwipe,mbr-testimonials-slider,font-awesome\">\n\n    <mbr-parameters>\n        \n        <header>Size</header>\n        <input type=\"range\" title=\"Padding Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"2\" inline>\n        <input type=\"range\" title=\"Padding Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"2\" inline>\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubTitle\" checked>\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked>\n        <input type=\"checkbox\" title=\"Author\" name=\"showAuthor\">\n        <input type=\"checkbox\" title=\"Profession\" name=\"showProfession\">\n        <header>Color</header>\n        <input type=\"color\" title=\"Arrow Color\" name=\"arrow\" value=\"#354463\">\n        <input type=\"color\" title=\"Arrow Border Color\" name=\"arrowBorder\" value=\"#f0cda1\">\n        <input type=\"color\" title=\"Arrow Hover Border Color\" name=\"arrowBorderHover\" value=\"#354463\">\n        <input type=\"color\" title=\"Arrow Background Color\" name=\"arrowBg\" value=\"#354463\">\n\n        <header>Items</header>\n        <input type=\"color\" title=\"Quotes Color\" name=\"quotesColor\" value=\"#a1c8af\">\n        <input type=\"color\" title=\"Line Color\" name=\"lineColor\" value=\"#a1c8af\">\n        <input type=\"range\" inline title=\"Count Items\" name=\"testimonialsSlides\" min=\"1\" max=\"20\" step=\"1\" value=\"5\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"https://r.mobirisesite.com/932214/assets/images/court-h_m4rzmc2o.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/171547423\">\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"../_images/attorney.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#f0cda1\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay && bg.type !== 'color'\">\n        \n    </mbr-parameters>\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div class=\"container text-center\">\n        <h4 class=\"mbr-section-subtitle align-left mbr-fonts-style\" mbr-if=\"showSubTitle\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-section-subtitle\">About Us</h4>\n        <h3 class=\"mbr-section-title mbr-info align-left mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\"><span class=\"mbr-bold\">Our Story, Your Success</span></h3>\n        <div class=\"carousel-controls\">\n            <a class=\"carousel-control-prev\" role=\"button\" data-slide=\"prev\">\n                <span aria-hidden=\"true\" class=\"mobi-mbri mobi-mbri-arrow-prev mbr-iconfont sign icon-left\"></span>\n                <span class=\"sr-only\">Previous</span>\n            </a>\n\n            <a class=\"carousel-control-next\" role=\"button\" data-slide=\"next\">\n                <span aria-hidden=\"true\" class=\"mobi-mbri mobi-mbri-arrow-next mbr-iconfont sign icon-right\"></span>\n                <span class=\"sr-only\">Next</span>\n            </a>\n        </div>\n        <div class=\"carousel slide\" data-ride=\"carousel\" data-interval=\"20000\" role=\"listbox\">\n            <div mbr-list mbr-list-grow=\"testimonialsSlides\" class=\"carousel-inner\">\n                <div class=\"carousel-item\">\n                    <div class=\"col-md-12 col-lg-12 align-left block-bottom\">\n\n                        <div class=\"wrapper-block\">\n                            <div class=\"testimonials-slider__icon\">â€œ</div>\n                            <div class=\"text-block\">\n                                <p class=\"mbr-text mbr-light align-left mbr-fonts-style box\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .mbr-section-btn\">At Certus, we transform complex financial data into actionable insights while providing tailored strategies and ethical guidance. Combining local expertise with global best practices, we empower organizations to achieve transparency, compliance, and long-term success.<br><br>\n                                </p>\n                                <div class=\"author-box\">\n                                    <div class=\"align-left\">\n                                        <h6 class=\"author mbr-semibold pb-2 mbr-fonts-style line\" mbr-theme-style=\"display-7\" mbr-if=\"showAuthor\" data-app-selector=\".author\">(1) Ramphis Tromp,\n                                        </h6>\n                                        <h6 class=\"mbr-semibold profession mbr-fonts-style mbr-info\" mbr-theme-style=\"display-7\" mbr-if=\"showProfession\" data-app-selector=\".profession\">Director<br><br><br><br>\n                                        </h6>\n                                    </div>\n                                </div>\n            